<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/fon">
        <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/usimage"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:padding="3dp"
                android:scaleType="centerCrop"
                app:layout_constraintStart_toStartOf="parent"
                app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.CornerSize50Percent"
                app:srcCompat="@drawable/ic_profile_circle"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="32dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"/>
        <TextView
                android:id="@+id/txtsetfullname"
                android:text="@string/connect"
                android:layout_width="wrap_content"
                app:fontFamily="@font/fredoka_fonts"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@+id/usimage"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@+id/usimage"
                app:layout_constraintEnd_toEndOf="@+id/usimage"/>
        <TextView
                android:id="@+id/txtsetemail"
                android:text="@string/connect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:fontFamily="@font/fredoka_fonts"
                app:layout_constraintTop_toBottomOf="@+id/txtsetfullname"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toStartOf="@+id/txtsetfullname"
                app:layout_constraintEnd_toEndOf="@+id/txtsetfullname"/>
        <View
                android:id="@+id/view11"
                android:layout_width="350dp"
                android:layout_height="45dp"
                android:background="@drawable/walletcard"
                app:layout_constraintStart_toStartOf="@+id/view14"
                app:layout_constraintEnd_toEndOf="@+id/view14"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/view32"
                android:layout_marginTop="12dp"/>
        <View
                android:id="@+id/view32"
                android:layout_width="350dp"
                android:layout_height="93dp"
                android:background="@drawable/walletcard"
                app:layout_constraintTop_toBottomOf="@+id/view9"
                android:layout_marginTop="12dp"
                app:layout_constraintStart_toStartOf="@+id/view14"/>
        <View
                android:id="@+id/view30"
                android:layout_width="350dp"
                android:layout_height="45dp"
                android:background="@drawable/walletcard"
                app:layout_constraintTop_toBottomOf="@+id/txtsetemail"
                android:layout_marginTop="28dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.508"/>
        <View
                android:id="@+id/view31"
                android:layout_width="350dp"
                android:layout_height="45dp"
                android:background="@drawable/walletcard"
                app:layout_constraintTop_toBottomOf="@+id/view30"
                android:layout_marginTop="12dp"
                app:layout_constraintStart_toStartOf="@+id/view30"
                app:layout_constraintEnd_toEndOf="@+id/view30"
                app:layout_constraintHorizontal_bias="0.0"/>
        <View
                android:id="@+id/view9"
                android:layout_width="350dp"
                android:layout_height="141dp"
                android:background="@drawable/walletcard"
                app:layout_constraintStart_toStartOf="@+id/view31"
                app:layout_constraintEnd_toEndOf="@+id/view31"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/view31"
                android:layout_marginTop="12dp"/>
        <View
                android:id="@+id/view13"
                android:layout_width="350dp"
                android:layout_height="3dp"
                android:background="@color/fon"
                app:layout_constraintTop_toTopOf="@+id/view9"
                app:layout_constraintStart_toStartOf="@+id/view9"
                app:layout_constraintEnd_toEndOf="@+id/view9"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintVertical_bias="0.653"
                app:layout_constraintBottom_toTopOf="@+id/view16"/>
        <View
                android:id="@+id/view14"
                android:layout_width="350dp"
                android:layout_height="3dp"
                android:background="@color/fon"
                app:layout_constraintTop_toTopOf="@+id/view16"
                app:layout_constraintBottom_toBottomOf="@+id/view9"
                app:layout_constraintEnd_toEndOf="@+id/view9"
                app:layout_constraintStart_toStartOf="@+id/view9"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintVertical_bias="0.367"/>
        <View
                android:id="@+id/view16"
                android:layout_width="350dp"
                android:layout_height="3dp"
                app:layout_constraintTop_toTopOf="@+id/view9"
                app:layout_constraintBottom_toBottomOf="@+id/view9"
                app:layout_constraintStart_toStartOf="@+id/view9"
                app:layout_constraintEnd_toEndOf="@+id/view9"/>
        <ImageView
                android:id="@+id/imageView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_security_user"
                app:layout_constraintTop_toTopOf="@+id/view9"
                app:layout_constraintStart_toStartOf="@+id/view13"
                android:layout_marginStart="16dp"
                app:layout_constraintBottom_toTopOf="@+id/view13"
                tools:ignore="ContentDescription"/>
        <ImageView
                android:id="@+id/imageView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_teacher"
                app:layout_constraintStart_toStartOf="@+id/imageView16"
                app:layout_constraintTop_toBottomOf="@+id/view13"
                app:layout_constraintBottom_toTopOf="@+id/view14"
                tools:ignore="ContentDescription"/>
        <ImageView
                android:id="@+id/imageView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_emptywallet"
                app:layout_constraintStart_toStartOf="@+id/imageView17"
                app:layout_constraintTop_toBottomOf="@+id/view14"
                app:layout_constraintBottom_toBottomOf="@+id/view9"
                tools:ignore="ContentDescription"/>
        <ImageView
                android:id="@+id/imageView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_logout"
                app:layout_constraintTop_toTopOf="@+id/view11"
                app:layout_constraintBottom_toBottomOf="@+id/view11"
                tools:ignore="ContentDescription" app:layout_constraintStart_toStartOf="@+id/imageView18"/>
        <TextView
                android:id="@+id/txtsettperinfo"
                android:text="@string/connect"
                android:layout_width="279dp"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                app:fontFamily="@font/fredoka_fonts"
                app:layout_constraintTop_toTopOf="@+id/view9"
                app:layout_constraintBottom_toTopOf="@+id/view13"
                app:layout_constraintStart_toStartOf="@+id/txtsettshop"/>
        <TextView
                android:id="@+id/txtsettperinfo2"
                android:text="@string/connect"
                android:layout_width="279dp"
                android:layout_height="45dp"
                app:fontFamily="@font/fredoka_fonts"
                android:gravity="center_vertical"
                app:layout_constraintTop_toBottomOf="@+id/view13"
                app:layout_constraintBottom_toTopOf="@+id/view14"
                app:layout_constraintStart_toStartOf="@+id/txtsettperinfo"/>
        <TextView
                android:id="@+id/txtsettperinfo3"
                android:text="@string/connect"
                android:layout_width="279dp"
                app:fontFamily="@font/fredoka_fonts"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                app:layout_constraintTop_toBottomOf="@+id/view14"
                app:layout_constraintBottom_toBottomOf="@+id/view9"
                app:layout_constraintStart_toStartOf="@+id/txtsettperinfo2"/>
        <TextView
                android:id="@+id/txtsettshop"
                android:text="@string/astrum_store"
                android:layout_width="286dp"
                app:fontFamily="@font/fredoka_fonts"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                app:layout_constraintTop_toTopOf="@+id/view31"
                app:layout_constraintStart_toEndOf="@+id/imageView14"
                android:layout_marginStart="12dp"/>
        <TextView
                android:id="@+id/txtsettpassword"
                android:text="@string/change_password"
                android:layout_width="266dp"
                app:fontFamily="@font/fredoka_fonts"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                app:layout_constraintBottom_toTopOf="@+id/view34"
                app:layout_constraintTop_toTopOf="@+id/view32"
                app:layout_constraintStart_toStartOf="@+id/txtsettperinfo"/>
        <TextView
                android:id="@+id/txtsettpassword2"
                android:text="@string/app_password"
                android:layout_width="266dp"
                app:fontFamily="@font/fredoka_fonts"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                app:layout_constraintTop_toBottomOf="@+id/view34"
                app:layout_constraintBottom_toBottomOf="@+id/view32"
                app:layout_constraintStart_toStartOf="@+id/txtsettpassword"/>
        <TextView
                android:id="@+id/txtsetlogout"
                android:text="@string/log_out"
                android:layout_width="276dp"
                app:fontFamily="@font/fredoka_fonts"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                android:textColor="#FF0000"
                app:layout_constraintTop_toTopOf="@+id/view11"
                app:layout_constraintBottom_toBottomOf="@+id/view11"
                app:layout_constraintStart_toStartOf="@+id/txtsettpassword"/>
        <ProgressBar
                android:id="@+id/setprogressBar"
                style="?android:attr/progressBarStyle"
                android:layout_width="80dp"
                android:layout_height="81dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        <View
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:id="@+id/view"
                android:background="@drawable/bottommenu"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
        <View
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:id="@+id/view2"
                app:layout_constraintStart_toStartOf="parent"/>
        <ImageView
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:scaleType="fitXY"
                android:elevation="0dp"
                app:srcCompat="@drawable/ic_transfer_wallet"
                android:id="@+id/imageView8"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/view2"
                app:layout_constraintTop_toTopOf="@+id/view"
                tools:ignore="ContentDescription"/>
        <ImageView
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:scaleType="fitXY"
                android:elevation="0dp"
                app:srcCompat="@drawable/ic_discover"
                android:id="@+id/imageView9"
                app:layout_constraintTop_toTopOf="@+id/imageView8"
                app:layout_constraintEnd_toStartOf="@+id/imageView10"
                app:layout_constraintStart_toEndOf="@+id/imageView8"
                tools:ignore="ContentDescription"/>
        <View
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:id="@+id/view5"/>
        <ImageView
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_setting"
                android:id="@+id/imageView10"
                app:layout_constraintTop_toTopOf="@+id/imageView9"
                android:elevation="0dp"
                app:layout_constraintEnd_toStartOf="@+id/view5"
                tools:ignore="ContentDescription"/>
        <TextView
                android:text="@string/student_ranks"
                android:layout_width="269dp"
                android:layout_height="45dp"
                android:gravity="center_vertical"
                app:fontFamily="@font/fredoka_fonts"
                android:id="@+id/txtsettrank"
                app:layout_constraintStart_toStartOf="@+id/txtsettshop"
                app:layout_constraintTop_toTopOf="@+id/view30"
                app:layout_constraintBottom_toBottomOf="@+id/view30"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_raks" android:id="@+id/imageView13"
                app:layout_constraintStart_toStartOf="@+id/imageView16"
                app:layout_constraintBottom_toBottomOf="@+id/view30"
                app:layout_constraintTop_toTopOf="@+id/view30"
                tools:ignore="ContentDescription"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_shop11" android:id="@+id/imageView14"
                app:layout_constraintStart_toStartOf="@+id/imageView13"
                app:layout_constraintTop_toTopOf="@+id/view31"
                app:layout_constraintBottom_toBottomOf="@+id/view31"
                tools:ignore="ContentDescription"/>
        <View
                android:layout_width="350dp"
                android:layout_height="3dp"
                android:background="@color/fon"
                android:id="@+id/view34"
                app:layout_constraintTop_toTopOf="@+id/view32"
                app:layout_constraintBottom_toBottomOf="@+id/view32"
                app:layout_constraintStart_toStartOf="@+id/view32"
                app:layout_constraintEnd_toEndOf="@+id/view32"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_key_square"
                android:id="@+id/imageView15"
                app:layout_constraintTop_toTopOf="@+id/view32"
                app:layout_constraintStart_toStartOf="@+id/imageView18"
                app:layout_constraintBottom_toTopOf="@+id/view34"
                tools:ignore="ContentDescription"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_lock" android:id="@+id/imageView19"
                app:layout_constraintTop_toBottomOf="@+id/view34"
                app:layout_constraintStart_toStartOf="@+id/imageView15"
                app:layout_constraintBottom_toBottomOf="@+id/view32"
                tools:ignore="ContentDescription"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>