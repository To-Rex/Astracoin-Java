<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:background="@drawable/bottommenu1"
        style="@style/custombottomsheet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="WebViewLayout">
        <View
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:id="@+id/view38"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="0dp"
                app:layout_constraintHorizontal_bias="0.0"/>
        <View
                android:layout_width="80dp"
                android:layout_height="8dp"
                android:background="@drawable/shadovfon"
                android:id="@+id/onclossed1"
                app:layout_constraintBottom_toBottomOf="@+id/view38"
                app:layout_constraintEnd_toEndOf="@+id/view38"
                app:layout_constraintStart_toStartOf="@+id/view38"
                app:layout_constraintTop_toTopOf="@+id/view38"/>
        <LinearLayout
                android:layout_width="0dp"
                android:layout_height="600dp"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@+id/view38"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent">
            <WebView
                    android:id="@+id/webView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintTop_toBottomOf="@+id/onclossed1"/>
        </LinearLayout>

        <View
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/view39"
                android:background="@drawable/loginedittext3"
                android:layout_y="24dp"
            android:visibility="invisible"
                android:layout_x="9dp"
                app:layout_constraintEnd_toEndOf="@+id/view38"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginEnd="16dp"/>
        <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp"
            android:visibility="invisible"
                android:src="@drawable/ic_close_square"
                app:layout_constraintEnd_toEndOf="@+id/view39"
                app:layout_constraintStart_toStartOf="@+id/view39"
                app:layout_constraintBottom_toBottomOf="@+id/view39"
                app:layout_constraintTop_toTopOf="@+id/view39"
                tools:ignore="ContentDescription" android:id="@+id/onclossed"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>