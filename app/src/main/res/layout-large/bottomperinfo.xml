<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bottombar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:id="@+id/view"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <View
            android:layout_width="80dp"
            android:layout_height="6dp"
            android:background="@drawable/shadovfon"
            android:id="@+id/view7"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
            android:id="@+id/ttt1"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_1"
            android:textSize="40sp"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/viev1" android:layout_marginTop="68dp"/>
        <TextView
            android:id="@+id/ttt2"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_2"
            android:textSize="40sp"
            app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="@+id/guideline4" app:layout_constraintTop_toTopOf="@+id/ttt1"/>
        <TextView
            android:id="@+id/ttt3"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            android:textColor="@color/black"
            app:fontFamily="@font/fredoka_fonts"
            android:text="@string/_3"
            android:textSize="40sp"
            app:layout_constraintTop_toTopOf="@+id/ttt2"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"/>
        <TextView
            android:id="@+id/ttt4"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_4"
            android:textSize="40sp"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/ttt1"
            android:layout_marginTop="44dp"
            app:layout_constraintEnd_toEndOf="@+id/ttt1"/>
        <TextView
            android:id="@+id/ttt5"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_5"
            android:textSize="40sp"
            app:layout_constraintStart_toStartOf="@+id/ttt2" app:layout_constraintTop_toTopOf="@+id/ttt4"
            app:layout_constraintEnd_toEndOf="@+id/ttt2"/>
        <TextView
            android:id="@+id/ttt6"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_6"
            android:textSize="40sp"
            app:layout_constraintTop_toTopOf="@+id/ttt5" app:layout_constraintEnd_toEndOf="@+id/ttt3"
            app:layout_constraintStart_toStartOf="@+id/ttt3"/>
        <TextView
            android:id="@+id/ttt7"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_7"
            android:textSize="40sp"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/ttt4"
            android:layout_marginTop="44dp"
            app:layout_constraintEnd_toEndOf="@+id/ttt4"/>
        <TextView
            android:id="@+id/ttt8"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_8"
            android:textSize="40sp"
            app:layout_constraintStart_toStartOf="@+id/ttt5"
            app:layout_constraintTop_toTopOf="@+id/ttt7"
            app:layout_constraintEnd_toEndOf="@+id/ttt5"/>
        <TextView
            android:id="@+id/ttt9"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            app:fontFamily="@font/fredoka_fonts"
            android:textColor="@color/black"
            android:text="@string/_9"
            android:textSize="40sp"
            app:layout_constraintTop_toTopOf="@+id/ttt8"
            app:layout_constraintStart_toStartOf="@+id/ttt6"
            app:layout_constraintEnd_toEndOf="@+id/ttt6"/>
        <TextView
            android:id="@+id/ttt0"
            android:layout_width="64dp"
            android:layout_height="70dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:text="@string/_0"
            app:fontFamily="@font/fredoka_fonts"
            android:textSize="40sp"
            app:layout_constraintTop_toBottomOf="@+id/ttt8"
            android:layout_marginTop="44dp"
            app:layout_constraintEnd_toEndOf="@+id/ttt8"
            app:layout_constraintStart_toStartOf="@+id/ttt8"/>
        <ImageView
            android:id="@+id/imgbackk"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:srcCompat="@drawable/ic_backbutton"
            tools:ignore="ContentDescription"
            app:layout_constraintTop_toTopOf="@+id/ttt0"
            app:layout_constraintBottom_toBottomOf="@+id/ttt0"
            app:layout_constraintStart_toStartOf="@+id/ttt9"
            app:layout_constraintEnd_toEndOf="@+id/ttt9"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="350dp"
            tools:layout_editor_absoluteX="0dp"
            app:layout_constraintTop_toBottomOf="@+id/ttt0"/>
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_percent="0.80"
            android:id="@+id/guideline2"/>
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_percent="0.20"
            android:id="@+id/guideline3"/>
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_percent="0.5"
            android:id="@+id/guideline4"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/view33"
            app:layout_constraintTop_toTopOf="@+id/viev2"
            app:layout_constraintEnd_toStartOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="@+id/guideline4"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/view35"
            app:layout_constraintStart_toEndOf="@+id/viev3"
            app:layout_constraintTop_toTopOf="@+id/viev3"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/view36"
            app:layout_constraintEnd_toStartOf="@+id/viev2"
            app:layout_constraintTop_toTopOf="@+id/viev1"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/shadov"
            app:layout_constraintEnd_toStartOf="@+id/view36"
            android:id="@+id/viev1"
            app:layout_constraintTop_toTopOf="@+id/guideline5"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/shadov"
            android:id="@+id/viev2"
            app:layout_constraintEnd_toStartOf="@+id/view33"
            app:layout_constraintTop_toTopOf="@+id/view36"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/shadov"
            android:id="@+id/viev3"
            app:layout_constraintStart_toEndOf="@+id/view33"
            app:layout_constraintTop_toTopOf="@+id/view33"/>
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/shadov"
            android:id="@+id/viev4"
            app:layout_constraintStart_toEndOf="@+id/view35"
            app:layout_constraintTop_toTopOf="@+id/view35"/>
        <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_percent="0.22"
            android:id="@+id/guideline5"/>
        <TextView
            android:id="@+id/txtpassinfo"
            android:text="@string/your_password"
            android:textColor="@color/black"
            app:fontFamily="@font/fredoka_fonts"
            android:textSize="40sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/view33"
            android:layout_marginBottom="68dp"
            app:layout_constraintEnd_toEndOf="@+id/viev4"
            app:layout_constraintStart_toStartOf="@+id/viev1" app:layout_constraintHorizontal_bias="0.49"/>
        <EditText
            android:id="@+id/edinumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            app:fontFamily="@font/fredoka_fonts"
            android:visibility="gone"
            android:ems="10"
            android:maxLength="4"
            app:layout_constraintBottom_toTopOf="@+id/ttt2"
            app:layout_constraintTop_toBottomOf="@+id/view33"
            app:layout_constraintEnd_toEndOf="@+id/viev4"
            app:layout_constraintStart_toStartOf="@+id/viev1"
            tools:ignore="Autofill,LabelFor"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>